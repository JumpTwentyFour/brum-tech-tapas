{
  "name": "brum-tech-tapas",
  "version": "1.0.0",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.js --mode development && npm run copy-svgs && npm run copy-icons && webpack --watch --progress",
    "build": "webpack --mode production && npm run copy-svgs && npm run copy-icons",
    "copy-svgs": "rm -rf dist/svgs && cp -R src/svgs dist/svgs",
    "copy-icons": "rm -rf dist/icons && cp -R src/icons dist/icons",
    "standard": "standard"
  },
  "eslintConfig": {
    "extends": "airbnb"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.1.4",
    "css-loader": "^2.1.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "image-webpack-loader": "^5.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "regenerator-runtime": "^0.13.2",
    "sass-loader": "^7.1.0",
    "standard": "^12.0.1",
    "style-loader": "^0.23.1",
    "svg-inline-loader": "^0.8.0",
    "terser-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.4.1"
  },
  "standard": {
    "env": [
      "node",
      "es6"
    ]
  }
}
